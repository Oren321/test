local FredHUb = loadstring(game:HttpGet("https://raw.githubusercontent.com/idonthaveoneatm/Libraries/normal/quake/src"))() -- uh yes we are just getting the library

-- creating the window 
local Main = FredHUb:Window({ --
    Title = "Private Script | By Oren",
})

-- creating the tab
local Tab = Main:Tab({
    Name = "AutoFarm",
    tabColor = Color3.fromHex("#71d1f5"),
    Image = ""
})

local EventTab = Main:Tab({
    Name = "Farming Event",
    tabColor = Color3.fromHex("#71d1f5"),
    Image = "rbxassetid://10709769841"
})
-- you can add more tab! as neeeded

-- creating the toggle
Tab:Toggle({
    Name = "This is a toggle!",
    Default = false,
    Callback = function(Value)
        print(Value)
    end    
})

EventTab:Toggle({
    Name = "This is a toggle!",
    Default = false,
    Callback = function(Value)
        print(Value)
    end    
})

-- creating the textbox
Tab:TextBox({
    Name = "Text Box!",
    Callback = function(value)
        print(value)
    end
})


EventTab:TextBox({
    Name = "Text Box!",
    Callback = function(value)
        print(value)
    end
})

-- creating the label
Tab:Label("Freaky Fred")

Tab:Slider({
    Name = "Slider!",
    Min = 1,
    Max = 99,
    Step = 1,
    InitialValue = 1,
    Callback = function(Value)
        print(Value)
    end
})

-- creating the button
Tab:Button({
    Name = "Button!",
    Callback = function()
          print("button pressed")
      end    
})

-- creating the dropdown
Tab:Dropdown({
    Name = "Dropdown",
    Default = "1",
    Items = {"1", "2"},
    Multiselect = true, -- So they can select multiple options
    Callback = function(Value)
        print(Value)
    end    
})
